import java.util.Scanner;
public class number{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt();
        if(num>0)
        System.out.println("Positive");
        if(num<0)
        System.out.println("Negative");
        if(num==0)
        System.out.println("zero");
    }
}
