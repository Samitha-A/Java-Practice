import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int num=n,rev=0,digit;
        while(num>0){
            digit=num%10;
            rev=rev*10+digit;
            num/=10;
        }
        if(rev==n){
            System.out.println("true");
        }
        else
        System.out.println("false");
    }
}
