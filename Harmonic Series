import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        double sum=0.00;
        double term=0.00;
        for(int i=1; i<=n; i++){
            term=1.00/i;
            sum+=term;
        }
        System.out.printf("%.2f",sum);
    }
}
