import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int sum=0;
        for(int i=1; i<=n; i++){
            sum+=i*i;
        }
        if(sum==0){
            System.out.println("Invalid");
        }
        else System.out.println(sum);
    }
}
